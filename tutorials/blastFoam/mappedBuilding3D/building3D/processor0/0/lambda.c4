FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0";
    object      lambda.c4;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    ground
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            calculated;
        value           uniform 0;
    }
    internalPatch
    {
        type            internal;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform 0;
    }
}



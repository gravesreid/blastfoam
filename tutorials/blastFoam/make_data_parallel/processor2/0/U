FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    ground
    {
        type            slip;
    }
    outlet
    {
        type            zeroGradient;
    }
    wall
    {
        type            slip;
    }
    wall2
    {
        type            slip;
    }
    wall3
    {
        type            slip;
    }
    internalPatch
    {
        type            internal;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary2to6
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary2to7
    {
        type            processor;
        value           uniform (0 0 0);
    }
}



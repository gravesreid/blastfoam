FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0.0001";
    object      overpressure;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    ground
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    wall
    {
        type            calculated;
        value           uniform 0;
    }
    wall2
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    wall3
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    internalPatch
    {
        type            internal;
    }
    procBoundary4to2
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary4to6
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary4to7
    {
        type            processor;
        value           uniform 0;
    }
}



FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    cylinder
    {
        type            slip;
    }
    internalPatch
    {
        type            internal;
    }
    procBoundary5to1
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary5to2
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary5to3
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary5to4
    {
        type            processor;
        value           uniform (0 0 0);
    }
}



    {
        type            zeroGradient;
    }
    internalPatch
    {
        type            internal;
    }
    procBoundary3to0
    {
        type            processor;
        value           uniform 300;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 300;
    }
    procBoundary3to4
    {
        type            processor;
        value           uniform 300;
    }
    procBoundary3to5
    {
        type            processor;
